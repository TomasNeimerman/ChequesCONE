{
  "name": "chequesbanco",
  "version": "1.0.0",
  "productName": "BerjermanErpUpdater",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^31.5.0",
    "electron-builder": "^25.0.5",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "chequesbanco": "file:",
    "express": "^2.5.11",
    "mssql": "^11.0.1",
    "tedious": "^18.6.1",
    "xlsx": "^0.18.5"
  },
  "build": {
    "appId": "com.bejermanErp.updater",
    "productName": "BejermanErpUpdater",
    "files": [
      "**/*",
      "main.js"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
